  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lra8dev/hoverdocs-ai"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.28.1",
  "devDependencies": {
    "@hoverdocs/eslint-config": "workspace:*",
